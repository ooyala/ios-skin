{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "flowtype",
    "import",
    "jsx-a11y",
    "react"
  ],
  "rules": {
    "max-len": [
      "error",
      {
        "code": 120
      }
    ]
  },
  "settings": {
    "flowtype": {
      "onlyFilesWithFlowAnnotation": false
    }
  }
}
